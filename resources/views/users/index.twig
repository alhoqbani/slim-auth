{% extends 'layouts/master.twig' %}

{% block content %}
    <div class="col-sm-6 col-sm-offset-3">
        <h2>Users</h2>
        <ul>
            {% for user in users %}
                <a href="{{ path_for('users.show', {id:user.id}) }}">
                    <li>{{ user.name }}</li>
                </a>
            {% endfor %}
        </ul>

    </div>

{% endblock %}